{
    "category": "ViewDNS",
    "description": "Takes a nameserver and quickly shows all other domains that share the same nameserver.",
    "enabled": false,
    "name": "ViewDNS Reverse NS",
    "pap": "red",
    "regexPatterns": [
        "(?:[a-z0-9](?:[a-z0-9-]{0,61}[a-z0-9])?\\.)+[a-z0-9][a-z0-9-]{0,61}[a-z0-9]"    
    ],
    "urls": [
        "https://viewdns.info/reversens/?ns={SELECTION_TEXT_AREA}"
    ]
}